{
  "name": "sgk/barcode-bundle",
  "type": "symfony-bundle",
  "description": "Symfony2 Barcode Generator Bundle with Twig function extension",
  "keywords": [
    "Symfony2",
    "barcode",
    "Symfony",
    "Bundle",
    "Twig",
    "generator",
    "qrcode",
    "qr code"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SGK\\BarcodeBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SGK\\BarcodeBundle\\Tests\\": "Tests/"
    }
  },
  "authors": [
    {
      "name": "Dinesh Rabara",
      "email": "dinesh.rabara@gmail.com"
    },
    {
      "name": "SHANG Guokan",
      "email": "shangguokan@hotmail.com",
      "homepage": "https://github.com/shangguokan"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "twig/twig": "^1.26 || ^2.0"
  },
  "suggest": {
    "symfony/twig-bundle": "^4.0 || ^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.0",
    "g-1-a/composer-test-scenarios": "^2.0",
    "symfony/config": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/http-kernel": "^4.0",
    "symfony/options-resolver": "^4.0"
  },
  "minimum-stability": "stable",
  "scripts": {
    "test": "phpunit",
    "scenario": "scenarios/install",
    "post-update-cmd": [
      "create-scenario symfony4 'symfony/config:^4.0 symfony/dependency-injection:^4.0 symfony/http-kernel:^4.0 symfony/options-resolver:^4.0'",
      "create-scenario symfony5 'symfony/config:^5.0 symfony/dependency-injection:^5.0 symfony/http-kernel:^5.0 symfony/options-resolver:^5.0'",
      "dependency-licenses"
    ]
  },
  "config": {
    "vendor-dir": "../../vendor"
  }
}
